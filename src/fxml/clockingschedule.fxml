                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <columns>
                  <TableColumn fx:id="clockingScheduleDefinition" prefWidth="329.0" text="CLOCKING SCHEDULE" />
               </columns>
            </TableView>
         </content>
      </ScrollPane>
      <HBox alignment="TOP_RIGHT" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" onAction="#addClockingSchedule" text="+" />
            <Button mnemonicParsing="false" onAction="#deleteClockingSchedule" prefWidth="25.0" text="-" />
